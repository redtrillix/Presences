{
	"name": "presences",
	"version": "3.0.0",
	"author": "Timeraa",
	"license": "MPL-2.0",
	"scripts": {
		"lint": "prettier --write . && eslint --fix .",
		"lint:ci": "prettier --check . && eslint .",
		"format": "npm run ms && npm run lint",
		"se": "npm run format && npm run bumpChanged uncommitted",
		"compileTools": "tsc -p util/tools/tsconfig.json",
		"inc": "npm run compileTools && node ./util/tools/massVer.js",
		"bumpChanged": "npm run compileTools && node ./util/tools/bumpChanged.js",
		"sv": "npm run compileTools && node ./util/tools/auto/schemaValidator.js",
		"pu": "npm run compileTools && node ./util/tools/auto/presenceUpdater.js",
		"ms": "npm run compileTools && node ./util/tools/metadataSorter.js",
		"compile": "npm run compileTools && node ./util/tools/compileChanged.js",
		"prepare": "is-ci || husky install",
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
	},
	"devDependencies": {
		"@babel/core": "7.19.1",
		"@babel/preset-env": "7.19.1",
		"@commitlint/cli": "17.1.2",
		"@commitlint/config-angular": "17.1.0",
		"@types/babel__core": "7.1.19",
		"@types/chrome": "0.0.197",
		"@types/debug": "4.1.7",
		"@types/glob": "8.0.0",
		"@types/json-to-ast": "2.1.2",
		"@types/node": "18.7.21",
		"@types/semver": "7.3.12",
		"@typescript-eslint/eslint-plugin": "5.38.0",
		"@typescript-eslint/parser": "5.38.0",
		"axios": "0.27.2",
		"chalk": "4.1.2",
		"debug": "4.3.4",
		"eslint": "8.24.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-no-one-time-vars": "2.4.2",
		"eslint-plugin-unicorn": "43.0.2",
		"glob": "8.0.3",
		"husky": "8.0.1",
		"is-ci": "3.0.1",
		"json-to-ast": "2.1.0",
		"jsonschema": "1.4.1",
		"mongodb": "4.10.0",
		"pmd": "github:PreMiD/pmd#1.0.3",
		"prettier": "2.7.1",
		"semver": "7.3.7",
		"source-map-support": "0.5.21",
		"terser": "5.15.0",
		"typescript": "4.8.3"
	}
}
