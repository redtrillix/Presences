{
	"$schema": "https://schemas.premid.app/metadata/1.7",
	"author": {
		"name": "Kyrie",
		"id": "368399721494216706"
	},
	"service": "AirSong",
	"description": {
		"en": "AirSong is a music server for your web browser. You can listen to your MP3 music stored on your device directly from your browser, over the air, no need to copy all the music from your device to your PC. The only requirement is to be under the same LAN (WIFI), both, the device and the PC that will run the browser."
	},
	"url": "localhost",
	"regExp": "192[.]168[.]1[.]([0-9]+)",
	"version": "1.0.3",
	"logo": "https://i.imgur.com/rLh25vA.png",
	"thumbnail": "https://i.imgur.com/Xr5RBAm.png",
	"color": "#0094CB",
	"category": "music",
	"tags": [
		"audio",
		"server",
		"streaming"
	],
	"settings": [
		{
			"id": "songDetail",
			"title": "Song Title row1",
			"icon": "fas fa-comment-alt-edit",
			"value": "%artist% - %title%",
			"placeholder": "Use %title% or %artist% or %album%"
		},
		{
			"id": "songState",
			"title": "Song Title row2",
			"icon": "fas fa-comment-alt-edit",
			"value": "%album%",
			"placeholder": "Use %title% or %artist% or %album%"
		},
		{
			"id": "time",
			"title": "Show timestamps",
			"icon": "fad fa-stopwatch",
			"value": 1,
			"values": [
				"Off",
				"Time Left",
				"Time Elapsed"
			]
		}
	]
}